{
    "openapi": "3.0.0",
    "info": {
        "title": "InsiderOne SMS Bulk Messaging API",
        "description": "High-performance bulk SMS system with Redis cache, queue management and idempotency. Handles 200K+ messages with rate limiting (50 req/min), automatic retry logic, and comprehensive monitoring.",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/api/v1",
            "description": "API V1 Endpoint"
        }
    ],
    "paths": {
        "/messages/sent": {
            "get": {
                "tags": [
                    "Messages"
                ],
                "summary": "Get sent messages",
                "operationId": "b87562c03a76fd5c5c778ef32f68b614",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1,
                            "example": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Items per page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 50,
                            "example": 50
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SentMessagesResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "MessageSend": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "message_id": {
                        "type": "integer",
                        "example": 5
                    },
                    "customer_id": {
                        "type": "integer",
                        "example": 42
                    },
                    "phone_number": {
                        "type": "string",
                        "example": "905551234567"
                    },
                    "message_content": {
                        "type": "string",
                        "example": "Hello! Your order has been shipped."
                    },
                    "status": {
                        "type": "string",
                        "enum": [
                            "pending",
                            "sent",
                            "failed"
                        ],
                        "example": "sent"
                    },
                    "webhook_message_id": {
                        "type": "string",
                        "example": "msg_65a0b12f2c9d4",
                        "nullable": true
                    },
                    "sent_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2026-01-19T14:30:00.000000Z"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2026-01-19T14:25:00.000000Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2026-01-19T14:30:00.000000Z"
                    }
                },
                "type": "object"
            },
            "PaginationMeta": {
                "properties": {
                    "current_page": {
                        "type": "integer",
                        "example": 1
                    },
                    "per_page": {
                        "type": "integer",
                        "example": 50
                    },
                    "total": {
                        "type": "integer",
                        "example": 250
                    },
                    "last_page": {
                        "type": "integer",
                        "example": 5
                    }
                },
                "type": "object"
            },
            "SentMessagesResponse": {
                "properties": {
                    "success": {
                        "type": "boolean",
                        "example": true
                    },
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/MessageSend"
                        }
                    },
                    "meta": {
                        "$ref": "#/components/schemas/PaginationMeta"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Messages",
            "description": "Messages"
        }
    ]
}